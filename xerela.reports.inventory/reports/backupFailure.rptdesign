            <property name="queryText">SELECT D.backup_status AS BACKUP_STATUS, count(*) AS CNT
 FROM device D, birt_resolved_devices BIRT
 WHERE D.device_id = BIRT.device_id
 AND BIRT.execution_id = ?  
 GROUP BY D.backup_status
</property>
