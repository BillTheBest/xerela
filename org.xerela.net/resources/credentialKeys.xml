<?xml version="1.0" encoding="UTF-8"?>
<!-- Defines the credential keys that will dictate how a UI displays the credentials -->
<credentialDefinitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:noNamespaceSchemaLocation="credentialKeys.xsd">
    <credentialKey name="username" displayName="Username" displayAsPassword="false" static="false"/>
    <credentialKey name="password" displayName="Password" displayAsPassword="true" static="false"/>
    <credentialKey name="enableUsername" displayName="Enable Username" displayAsPassword="false" static="false"/>
    <credentialKey name="enablePassword" displayName="Enable Password" displayAsPassword="true" static="false"/>
    <credentialKey name="roCommunityString" displayName="SNMP Get Community" displayAsPassword="true" static="false"/>
    <credentialKey name="rwCommunityString" displayName="SNMP Set Community" displayAsPassword="true" static="false"/>
    <credentialKey name="snmpUsername" displayName="SNMPv3 Authentication Username" displayAsPassword="false" static="false"/>
    <credentialKey name="snmpAuthPassword" displayName="SNMPv3 Authentication Password" displayAsPassword="true" static="false"/>
    <credentialKey name="snmpPrivPassword" displayName="SNMPv3 Privacy Password" displayAsPassword="true" static="false"/>
</credentialDefinitions>