                <structure>
                    <property name="position">6</property>
                    <property name="name">last_backup</property>
                    <property name="nativeName">LAST_BACKUP</property>
                    <property name="dataType">date-time</property>
                    <property name="nativeDataType">93</property>
                </structure>
            </list-property>
            <property name="queryText">SELECT D.hostname AS HOSTNAME,
 D.ip_address AS IP_ADDRESS,
 D.vendor_sw AS VENDOR_SW,
 D.backup_status AS BACKUP_STATUS,
 D.backup_message AS BACKUP_MESSAGE,
 D.last_backup AS LAST_BACKUP
 FROM device D, birt_resolved_devices BIRT
 WHERE D.backup_status IS NOT NULL
 AND D.backup_status != 'SUCCESS'
 AND D.device_id = BIRT.device_id
 AND BIRT.execution_id = ?
 ORDER BY D.ip_address</property>
        </oda-data-set>
